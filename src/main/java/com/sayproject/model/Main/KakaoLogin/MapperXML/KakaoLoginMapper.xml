<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sayproject.model.Main.KakaoLogin.MapperXML.KakaoLgoin">
	
	<select id="kakaoIdDuplicate" parameterType="string" resultType="int">
		SELECT count(id) FROM kakao_login WHERE email=#{memberObjectId}
	</select>
	
	<insert id="kakaoJoin" parameterType="com.sayproject.model.Main.KakaoLogin.KakaoJoin">
		INSERT INTO kakao_login (id,email,profile_image_url,nickname,isDefaultImage,thumbnailImageUrl)
		VALUES(
		#{memberObjectId},
		#{emailOrId},
		#{profileImageUrl},
		#{nickname},
		#{isDefaultImage},
		#{thumbnailImageUrl}
		) 
		
	</insert>
</mapper>